<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
<!-- 사전순으로 배치 -->
	<typeAliases>
		<typeAlias type="com.kh.tripply.banner.domain.Banner" 			alias="Banner"/>
		<typeAlias type="com.kh.tripply.free.domain.Free" 				alias="Free"/>	
		<typeAlias type="com.kh.tripply.member.domain.Member"   		alias="Member"/>
		<typeAlias type="com.kh.tripply.notice.domain.Notice"			alias="Notice"/>
		<typeAlias type="com.kh.tripply.party.domain.Party" 			alias="Party"/>
		<typeAlias type="com.kh.tripply.party.domain.PartyReply" 		alias="PartyReply"/>
		<typeAlias type="com.kh.tripply.plan.domain.Planner" 			alias="Planner" />
		<typeAlias type="com.kh.tripply.plan.domain.Plan"               alias="Plan" />
   	    <typeAlias type="com.kh.tripply.plan.domain.PlannerReply"       alias="PlannerReply" />
		<typeAlias type="com.kh.tripply.point.domain.Point" 			alias="Point"/>	
  		<typeAlias type="com.kh.tripply.review.domain.Review" 			alias="Review"/>
		<typeAlias type="com.kh.tripply.review.domain.ReviewReply"		alias="ReviewReply"/>	
		<typeAlias type="com.kh.tripply.trade.domain.Trade" 			alias="Trade"/>
		<typeAlias type="com.kh.tripply.trade.domain.TradeReply" 		alias="TradeReply"/>
		<typeAlias type="com.kh.tripply.message.domain.Message" 		alias="Message"/>
		<typeAlias type="com.kh.tripply.plan.domain.Plan"               alias="Plan" />
      <typeAlias type="com.kh.tripply.plan.domain.PlannerReply"         alias="PlannerReply" />
	</typeAliases>

	<mappers>
		 <mapper resource="mappers/banner-mapper.xml"/>
		 <mapper resource="mappers/free-mapper.xml"/>
		 <mapper resource="mappers/message-mapper.xml"/>
		 <mapper resource="mappers/member-mapper.xml"/>
		 <mapper resource="mappers/notice-mapper.xml"/>
		 <mapper resource="mappers/party-mapper.xml"/>
		 <mapper resource="mappers/planner-mapper.xml" />
		 <mapper resource="mappers/point-mapper.xml"/>
		 <mapper resource="mappers/review-mapper.xml"/>
		 <mapper resource="mappers/review-reply-mapper.xml" />
		 <mapper resource="mappers/trade-mapper.xml"/>
		 <mapper resource="mappers/trade-reply-mapper.xml"/>
	</mappers>

</configuration>